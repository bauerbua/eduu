{
  "version": 1,
  "projects": {
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/api/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "outputPath": "coverage/apps/api",
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "eduu": {
      "projectType": "application",
      "root": "apps/eduu",
      "sourceRoot": "apps/eduu/src",
      "prefix": "eduu",
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/eduu",
            "index": "apps/eduu/src/index.html",
            "main": "apps/eduu/src/main.ts",
            "polyfills": "apps/eduu/src/polyfills.ts",
            "tsConfig": "apps/eduu/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["apps/eduu/src/favicon.ico", "apps/eduu/src/assets"],
            "styles": ["apps/eduu/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/eduu/src/environments/environment.ts",
                  "with": "apps/eduu/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "eduu:build:production"
            },
            "development": {
              "browserTarget": "eduu:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "proxyConfig": "apps/eduu/proxy.conf.json"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/eduu/src/**/*.ts",
              "apps/eduu/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "outputPath": "coverage/apps/eduu",
            "jestConfig": "apps/eduu/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "eduu-e2e": {
      "root": "apps/eduu-e2e",
      "sourceRoot": "apps/eduu-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/eduu-e2e/cypress.json",
            "devServerTarget": "eduu:serve:development",
            "tsConfig": "apps/eduu-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "eduu:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/eduu-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  }
}
